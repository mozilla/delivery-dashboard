{
  "name": "dashboard",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "antd": "^2.13.3",
    "eslint-plugin-react": "^7.3.0",
    "gh-pages": "^1.0.0",
    "react": "^16.0.0",
    "react-dom": "^16.0.0",
    "react-redux": "^5.0.6",
    "react-scripts": "1.0.14",
    "redux": "^3.7.2",
    "redux-logger": "^3.0.6",
    "redux-saga": "^0.16.0",
    "redux-thunk": "^2.2.0"
  },
  "homepage": "https://mozilla.github.io/delivery-dashboard/",
  "scripts": {
    "start": "react-app-rewired start",
    "prestart": "yarn run version-file",
    "build": "react-app-rewired build",
    "prebuild": "yarn run version-file",
    "test": "NODE_ENV=test react-app-rewired test --env=jsdom --silent",
    "test-coverage": "yarn test --coverage --collectCoverageFrom=src/**/*js --collectCoverageFrom=!src/index.js --collectCoverageFrom=!src/registerServiceWorker.js",
    "eject": "react-app-rewired eject",
    "deploy": "npm run build && gh-pages --add --dist build/",
    "lint": "eslint *.js src",
    "lint-fix": "yarn lint --fix",
    "flow": "flow",
    "flow-coverage": "flow-coverage-report -i 'src/**/*.js' -x 'src/**/*.test.js' -t html -t text",
    "check": "yarn lint && yarn flow",
    "precheck": "yarn run version-file",
    "version-file": "./update_version.sh"
  },
  "devDependencies": {
    "babel-plugin-import": "^1.5.0",
    "codecov": "^2.3.0",
    "enzyme": "^3.0.0",
    "enzyme-adapter-react-16": "^1.0.0",
    "eslint": "^4.4.1",
    "eslint-config-prettier": "^2.3.0",
    "eslint-plugin-flowtype": "^2.35.0",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-node": "^5.1.1",
    "eslint-plugin-prettier": "^2.2.0",
    "eslint-plugin-promise": "^3.5.0",
    "flow-bin": "^0.56.0",
    "flow-coverage-report": "^0.3.0",
    "flow-typed": "^2.1.5",
    "prettier": "^1.5.3",
    "react-app-rewired": "^1.2.9",
    "react-test-renderer": "^16.0.0",
    "redux-mock-store": "^1.3.0"
  }
}
